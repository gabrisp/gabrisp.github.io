{
    "name": "wagnerwagner/merx-starterkit",
    "description": "Starterkit for Merx.",
    "license": "proprietary",
    "require": {
      "php": ">=7.4.0 <8.2.0"
    },
    "scripts": {
        "analyze": [
            "composer install",
            "@analyze:composer",
            "@analyze:psalm",
            "rm -rf vendor; rm composer.lock"
        ],
        "analyze:composer": "composer validate --strict",
        "analyze:psalm": "psalm --clear-cache; psalm"
    }
}
